<h1><strong><%= @list.name %></strong></h1>
<% @list.bookmarks.each do |bookmark|%>
<ul>
    <li><%= bookmark.movie.title %></li>
    <li><%= bookmark.comment %> <%= link_to "delete", bookmark_path(bookmark), data: {turbo_method: :delete, turbo_confirm: "Are your sure?"}  %> </li>
  <% end %>
    <%= link_to "Back to the lists", lists_path, class: "btn btn-primary mx-4"%>
    <%= link_to "Create a new bookmark", new_list_bookmark_path(@list), class: "btn btn-primary mx-4"%>
</ul>
